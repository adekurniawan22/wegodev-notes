{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"listNotes\",\n  data: function () {\n    return {\n      notes: []\n    };\n  },\n  props: {},\n  methods: {\n    editNote(id) {\n      let dataForm = this.notes.find(note => note.id === id);\n      dataForm.mode = \"update\";\n      this.emitter.emit('emitForm', dataForm);\n    },\n    getData() {\n      axios.get('http://localhost/wegodev-notes/note').then(response => {\n        console.log(response.data);\n        this.notes.push(response.data);\n      });\n    }\n  },\n  mounted() {\n    this.getData();\n    this.emitter.on('emitRemoveNote', data => {\n      let noteIndex = this.notes.findIndex(note => note.id === data.id);\n      this.notes.splice(noteIndex, 1);\n    });\n    this.emitter.on('emitUpdateNote', data => {\n      let noteIndex = this.notes.findIndex(note => note.id === data.id);\n      this.notes[noteIndex].title = data.title;\n      this.notes[noteIndex].description = data.description;\n    });\n    this.emitter.on('emitSaveNote', data => {\n      let newId = 0;\n      if (this.notes.length === 0) {\n        newId = 1;\n      } else {\n        newId = this.notes[this.notes.length - 1].id + 1;\n      }\n      let newNote = {\n        id: newId,\n        title: data.title,\n        description: data.description\n      };\n      this.notes.push(newNote);\n      this.editNote(newId);\n    });\n  }\n};","map":{"version":3,"names":["axios","name","data","notes","props","methods","editNote","id","dataForm","find","note","mode","emitter","emit","getData","get","then","response","console","log","push","mounted","on","noteIndex","findIndex","splice","title","description","newId","length","newNote"],"sources":["C:\\xampp\\htdocs\\wegodev-notes\\resources\\src\\components\\listNotes.vue"],"sourcesContent":["<template>\r\n  <div class=\"listNotes\">\r\n    <ul>\r\n      <li v-for=\"(row, index) in notes\" :key=\"index\">\r\n        <button 