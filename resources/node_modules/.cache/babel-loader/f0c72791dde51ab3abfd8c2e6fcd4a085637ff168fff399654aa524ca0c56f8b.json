{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// import HelloWorld from \"./components/HelloWorld.vue\";\nimport ListNotes from \"./components/listNotes.vue\";\nimport FormNotes from \"./components/formNotes.vue\";\nexport default {\n  name: \"App\",\n  data: function () {\n    return {\n      dataForm: {},\n      notes: [{\n        id: 1,\n        title: \"Wegodev\",\n        description: \"Ini isi wegodev\"\n      }, {\n        id: 2,\n        title: \"Super User\",\n        description: \"Ini isi super user\"\n      }]\n    };\n  },\n  components: {\n    ListNotes,\n    FormNotes\n  },\n  methods: {\n    newNotes() {},\n    editNote(id) {\n      this.dataForm = this.notes.find(note => note.id === id);\n    },\n    saveNote(title, description) {\n      let newId = 0;\n      if (this.notes.length === 0) {\n        newId = 1;\n      } else {\n        newId = this.notes[this.notes.length - 1].id + 1;\n      }\n      let newNote = {\n        id: newId,\n        title: title,\n        description: description\n      };\n      this.notes.push(newNote);\n    },\n    updateNote(id, title, description) {\n      let noteIndex = this.notes.findIndex(note => note.id === id);\n      this.notes[noteIndex].title = title;\n      this.notes[noteIndex].description = description;\n    }\n  }\n};","map":{"version":3,"names":["ListNotes","FormNotes","name","data","dataForm","notes","id","title","description","components","methods","newNotes","editNote","find","note","saveNote","newId","length","newNote","push","updateNote","noteIndex","findIndex"],"sources":["c:\\xampp\\htdocs\\wegodev-notes\\resources\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"kiri\">\n      <div class=\"logo\">\n        <a href=\"#\">\n          <h2>Wegodev</h2>\n          <span>notes</span>\n        </a>\n      </div>\n      <div class=\"frame-notes\">\n        <button @click=\"newNotes\" class=\"bg-success btn btn-new-note\">\n          + Note Baru\n    