{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: \"formNotes\",\n  props: {},\n  data: function () {\n    return {\n      id: 0,\n      title: \"\",\n      description: \"\",\n      mode: 'save'\n    };\n  },\n  methods: {\n    submitSave() {\n      let data = {\n        title: this.title,\n        description: this.description\n      };\n      this.emitter.emit('emitSaveNote', data);\n      axios.get('http://localhost/wegodev-notes/note').then(response => {\n        this.notes = response.data;\n      });\n    },\n    submitUpdate() {\n      let data = {\n        id: this.id,\n        title: this.title,\n        description: this.description\n      };\n      this.emitter.emit('emitUpdateNote', data);\n    },\n    submitRemove() {\n      let data = {\n        id: this.id\n      };\n      this.emitter.emit('emitRemoveNote', data);\n      this.id = 0;\n      this.title = \"\";\n      this.description = \"\";\n    }\n  },\n  mounted() {\n    this.emitter.on('emitForm', data => {\n      this.id = data.id;\n      this.title = data.title;\n      this.description = data.description;\n      this.mode = data.mode;\n    });\n  }\n};","map":{"version":3,"names":["axios","name","props","data","id","title","description","mode","methods","submitSave","emitter","emit","get","then","response","notes","submitUpdate","submitRemove","mounted","on"],"sources":["C:\\xampp\\htdocs\\wegodev-notes\\resources\\src\\components\\formNotes.vue"],"sourcesContent":["<template>\r\n  <div class=\"formNotes\">\r\n    <form>\r\n      <div class=\"menu\">\r\n        <button type=\"button\" @click=\"submitRemove\" class=\"bg-danger btn btn-delete\">Delete</button>\r\n        <button type=\"button\" @click=\"submitSave\" class=\"bg-success btn\" v-if=\"mode == 'save'\">Save</button>\r\n        <button type=\"button\" @click=\"submitUpdate\" class=\"bg-success btn\" v-if=\"mode == 'update'\"> Update </button>\r\n      </div>\r\n      <div div class=\" content\">\r\n        <input type=\"text\" class=\"text\" placeholder=\"id\" v-model=\"id\" />\r\n        <input type=\"text\" class=\"text\" placeholder=\"Title\" v-model=\"title\" />\r\n        <textarea class=\"text textarea\" placeholder=\"Tuliskan rencana kamu\" v-model=\"description\"></textarea>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script type=\"text/javascript\">\r\n\r\nimport axios from 'axios';\r\nexport default {\r\n  name: \"formNotes\",\r\n  props: {},\r\n  data: function () {\r\n    return { id: 0, title: \"\", description: \"\", mode: 'save' };\r\n  },\r\n  methods: {\r\n    submitSave() {\r\n      let data = {\r\n        title: this.title,\r\n        description: this.description\r\n      }\r\n      this.emitter.emit('emitSaveNote', data);\r\n\r\n      axios.get('http://localhost/wegodev-notes/note').then(response => {\r\n        this.notes = response.data\r\n      });\r\n    },\r\n    submitUpdate() {\r\n      let data = {\r\n        id: this.id,\r\n        title: this.title,\r\n        description: this.description\r\n      }\r\n      this.emitter.emit('emitUpdateNote', data);\r\n    },\r\n    submitRemove() {\r\n      let data = { id: this.id }\r\n      this.emitter.emit('emitRemoveNote', data);\r\n\r\n      this.id = 0;\r\n      this.title = \"\";\r\n      this.description = \"\";\r\n    },\r\n  },\r\n  mounted() {\r\n    this.emitter.on('emitForm', data => {\r\n      this.id = data.id;\r\n      this.title = data.title;\r\n      this.description = data.description;\r\n      this.mode = data.mode;\r\n    });\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.menu {\r\n  background: #f7f7f7;\r\n  padding: 10px 25px;\r\n  margin-bottom: 25px;\r\n  text-align: right;\r\n  border-bottom: 1px solid #e8e6e6;\r\n}\r\n\r\n.btn-delete {\r\n  margin-right: 10px;\r\n}\r\n\r\n.content {\r\n  padding: 0px 25px;\r\n}\r\n\r\n.text {\r\n  display: block;\r\n  width: 100%;\r\n  padding: 0px;\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  color: #2c3e50;\r\n 